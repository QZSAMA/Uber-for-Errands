<header class="intro-header" >
  <div class="container">
    <div class="row">
      <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
        <div class="site-heading">
          <h1>All our registered Users</h1>
          
        </div>
      </div>
    </div>
  </div>
</header>

<div class="container">
  <div class="row">
    <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1" >
      {{#each allUsers}}
        <div class="post-preview" >
          <a href="/users/{{this.userName}}">
            <h2 id="dark" class="post-title">
              {{this.userName}}
            </h2>
          </a>
          <p class="post-meta">{{this.firstName}} {{this.lastName}}</p>
        </div>
      
      {{/each}}
      
    </div>
  </div>
</div>
